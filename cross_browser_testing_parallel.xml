<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="FHC Trip" parallel="tests" thread-count="2" verbose="2">

    <test name="Testing with chrome">
        <parameter name="browser" value="chrome"></parameter>
        <packages>
            <package name="com.techproed.crossbrowsertests"></package>
        </packages>
    </test>

    <test name="Testing with chrome-headless">
        <parameter name="browser" value="chrome-headless"></parameter>
        <packages>
            <package name="com.techproed.crossbrowsertests"></package>
        </packages>
    </test>

</suite>